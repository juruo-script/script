local HttpService = game:GetService("HttpService")

local function detectHttpSpy()
    for _, gui in ipairs(game.CoreGui:GetChildren()) do
        if gui:IsA("ScreenGui") and gui.Name == "HttpSpy" then
            return true
        end
    end
    return false
end

local function blockHttpSpy()
    for _, gui in ipairs(game.CoreGui:GetChildren()) do
        if gui:IsA("ScreenGui") and gui.Name == "HttpSpy" then
            gui:Destroy()
            warn("HTTP Spy detected and removed!")
        end
    end
end

local function startAntiHttpSpy()
    while wait(1) do
        if detectHttpSpy() then
            blockHttpSpy()
        end
    end
end

startAntiHttpSpy()
